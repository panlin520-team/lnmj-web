package com.lnmj.backManage.controller.backend.maker;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

/**
 * @Author: yilihua
 * @Date: 2019/6/21 15:51
 * @Description:    创客理由
 */
@Controller
@RequestMapping("/maker")
public class MakerController {
    //限购类别
    @RequestMapping("/tomakertypemanage")
    public String tomakertypemanage() {
        return "maker/makerType";
    }

    //创客订单
    @RequestMapping("/tomakerordermanage")
    public String tomakerordermanage() {
        return "maker/makerOrder";
    }

    //创客产品
    @RequestMapping("/tomakerproductmanage")
    public String tomakerproductmanage() {
        return "maker/makerProduct";
    }

    //创客用户
    @RequestMapping("/tomakerusermanage")
    public String tomakerusermanage() {
        return "maker/makerUser";
    }

    //创客参数
    @RequestMapping("/tomakermanage")
    public String tomakermanage() {
        return "maker/makerMember";
    }


}
