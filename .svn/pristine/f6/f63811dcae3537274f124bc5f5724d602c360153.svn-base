package com.lnmj.backManage.Enum;

/**
 * @Author: yilihua
 * @Date: 2019/9/6 16:14
 * @Description: 业绩统计方式
 */
public enum AchievementMethodEnum {
    NUMBER_ROYALTY(1, "单个提成"),
    ALL_ACHIEVEMENT_ROYALTY(2, "总业绩提成"),
    ALL_ACHIEVEMENT_LADDER_ROYALTY(3, "总业绩阶梯提成"),
    NUMBER_LADDER_ROYALTY(4, "个数阶梯提成");
    private final int code;
    private final String desc;

    AchievementMethodEnum(int code, String desc) {
        this.code = code;
        this.desc = desc;
    }

    public int getCode() {
        return code;
    }

    public String getDesc() {
        return desc;
    }
}
